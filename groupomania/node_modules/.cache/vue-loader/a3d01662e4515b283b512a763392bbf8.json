A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Forum.vue","sourceRoot":"src/components","sourcesContent":["<script>\r\nimport axios from \"axios\"; \r\nimport FormData from 'form-data';\r\nexport default {\r\n\tname: 'Forum',\r\n    data() {\r\n      return {\r\n        donneesText : {\r\n            img: \"\",\r\n            message : \"\"\r\n        },\r\n        comment : \"\",\r\n        messagePost : \"\",\r\n        mediaPost : \"\",\r\n        datePost : \"\"\r\n      };\r\n    },\r\n    methods: {\r\n        addPost(e) {\r\n        //editer un post  editPost\r\n            e.preventDefault();\r\n\r\n            var formData = new FormData();\r\n            let img = document.getElementById('picture').files[0];\r\n            // methode fonctionne le seul problème est l'affichage des images\r\n            formData.append('picture', img);\r\n            formData.append('message', this.message);\r\n            //console.log(...formData);\r\n            console.log(img);\r\n            axios.post(\"http://localhost:3030/api/post\", formData, {\r\n                headers: {\r\n                    'Content-Type': 'multipart/form-data'\r\n                }\r\n            })\r\n            .then(function (response) {\r\n                console.log(response);\r\n            })\r\n            .catch(function (error) {\r\n                this.output = error;\r\n            });\r\n        },\r\n        envoiCommentaire(e) {\r\n        //partager un post\r\n            e.preventDefault();\r\n            const params = new URLSearchParams();\r\n            params.append('message', this.comment);\r\n            console.log(...params);\r\n            axios.post(\"http://localhost:3030/api/comment\", params)\r\n            .then(function (response) {\r\n                console.log(response.data);\r\n                console.log(response.status);\r\n                console.log(response.statusText);\r\n                console.log(response.headers);\r\n                console.log(response.config);\r\n            })\r\n            .catch(function (error) {\r\n                this.output = error;\r\n            });\r\n        },\r\n    },\r\n    mounted () {\r\n        axios.get(\"http://localhost:3030/api/post\")\r\n        .then(response => {\r\n            (this.info = response.data);\r\n            //console.log(response);\r\n            console.log(this.info);\r\n            for (let item of this.info) {\r\n                this.messagePost = item.message\r\n                this.mediaPost = item.media\r\n                this.datePost = item.date\r\n                console.log(this.messagePost);\r\n            document.getElementById(\"blocPost\").innerHTML += `\r\n                    <div class=\"user\">\r\n                        <img src=\"../../assets/user-circle-regular.png\" alt=\"iconeUser\" title=\"iconeUser\" />\r\n                        <p>Name of User</p>\r\n                        <p class=\"date\">${this.datePost}</p>\r\n                    </div>\r\n                    <div class =\"details\">\r\n                        <img src=\"${this.mediaPost}\" class=\"img\"/>\r\n                        <p>${this.messagePost}</p>\r\n                    </div>\r\n            `\r\n            }\r\n        })\r\n    }\r\n}\r\n</script>\r\n<template>\r\n  <div id=\"blocCentral\">\r\n    <div class=\"publishPost\">\r\n        <ul>\r\n            <li @click=\"submit\">\r\n                <img src=\"../../assets/quote-left-solid.png\" alt=\"icone\" title=\"icone\" />\r\n                <p>Share an update</p>\r\n            </li>\r\n\r\n            <li @click=\"submitAnswer\">\r\n                <img src=\"../../assets/edit-solid.png\" alt=\"icone\" title=\"icone\" />\r\n                <p>Share a post</p>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n    <div class=\"blocPost\">\r\n        <form enctype='multipart/form-data'>\r\n            <input type=\"text\" placeholder=\"What's on your mind?\" v-model=\"message\">\r\n            <div class=\"iconePicture\"></div>\r\n            <input id=\"picture\" accept=\"image/*\" type=\"file\">\r\n            <button type=\"button\" @click=\"addPost\">Poster</button>\r\n        </form>\r\n    </div>\r\n    <div class=\"filters\">\r\n        <label for=\"connections\">Connections Only</label>\r\n        <select name=\"postsFriends\" id=\"postsFriends\">\r\n            <option value=\"\"></option>\r\n            <option value=\"postsKey\">Posts Key</option>\r\n        </select>\r\n        <label for=\"popular\">Sort by Popular</label>\r\n        <!--afficher les posts les plus consultés-->\r\n        <select name=\"popular\" id=\"popular\">\r\n            <option value=\"\"></option>\r\n            <option value=\"popularPosts\">Popular Posts</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"posts\">\r\n        <div id=\"post\">\r\n            <div id=\"blocPost\">\r\n                <button type=\"submit\" @click=\"submit\">reply</button>\r\n                    <form>\r\n                        <input type=\"text\" placeholder=\"answer to post\" v-model=\"comment\">\r\n                        <button type=\"submit\" @click=\"envoiCommentaire\">envoi</button>\r\n                    </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<style lang=\"scss\">\r\n.forum {\r\n    width: 75%;\r\n    margin-top: 40px;\r\n}\r\n.iconePicture {\r\n    content: url(\"../../assets/camera-retro-solid.png\");\r\n    width: 6%;\r\n    margin: 0 10px;\r\n}\r\n.publishPost {\r\n    width: 90%;\r\n    margin: 0 auto;\r\n    ul {\r\n        display: flex;\r\n        justify-content: space-around;\r\n        list-style: none;\r\n        li {\r\n            display: flex;\r\n            align-items: center;\r\n                flex-direction: row-reverse;\r\n            img {\r\n                width: 6%;\r\n                margin: 0 10px;\r\n            }\r\n        }\r\n    }\r\n}\r\n#blocPost {\r\n    width: 85%;\r\n    margin: 0 10%;\r\n    box-shadow: 2px 2px 2em rgb(0 0 0 / 16%);\r\n    input {\r\n        width: 95%;\r\n        height: 100px;\r\n        border: none;\r\n        padding-left: 30px;\r\n    }\r\n    .moreOptions {\r\n        display: flex;\r\n        margin-top: 10px;\r\n        .iconePicture {\r\n            content: url(\"../../assets/camera-retro-solid.png\");\r\n            width: 2%;\r\n            height: 20px;\r\n        }\r\n        input {\r\n            padding: 0;\r\n        }\r\n        button {\r\n            padding: 8px;\r\n            color: white;\r\n            text-transform: uppercase;\r\n            font-weight: bold;\r\n            background-color: #ff5722;\r\n            border: none;\r\n            margin-left: auto;\r\n        }\r\n    }\r\n}\r\n.filters {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    width: 85%;\r\n    margin: 0 auto;\r\n    label {\r\n        margin: 20px 10px;\r\n    }\r\n    select{\r\n        margin: 20px;\r\n        width: 20px;\r\n        border: none;\r\n    }\r\n}\r\n.posts {\r\n    width: 85%;\r\n    margin: 0 10%;\r\n    box-shadow: 2px 2px 2em rgb(0 0 0 / 16%);\r\n    #post {\r\n        margin: 0 auto;\r\n        background-color: white;\r\n        .user {\r\n            display: flex;\r\n            align-items: center;\r\n                justify-content: end;\r\n            p {\r\n                bottom: 10px;\r\n            }\r\n            p.date {\r\n                margin-left: auto;\r\n                margin-right: 10px;\r\n            }\r\n        }\r\n        .blocPost {\r\n            margin : 10px auto;\r\n        }\r\n        .details {\r\n            display: flex;\r\n            justify-content: space-around;\r\n        }\r\n        .img {\r\n            position: relative;\r\n            right: 15px;\r\n            bottom: 10px;\r\n            width: 15%;\r\n        }\r\n        button {\r\n            padding: 10px 12px;\r\n            color: white;\r\n            text-transform: uppercase;\r\n            font-weight: bold;\r\n            background-color: #ff5722;\r\n            border: none;\r\n            position: relative;\r\n            left: 42%;\r\n            top: 10px;\r\n            border-radius: 20px;\r\n        }\r\n    }\r\n}\r\n</style>"]}]}