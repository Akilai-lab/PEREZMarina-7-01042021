AAA,KAAA,CAAA,YAAA,GAAA,MAAA,CAAA,MAAA;AACA,OAFA;;AAGA,MAAA,MAAA,CAAA,aAAA,CAAA,IAAA;AAEA,UAAA,MAAA,GAAA,IAAA,eAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA,KAAA,KAAA;AAEA,MAAA,KAAA,CAAA,IAAA,CAAA,gCAAA,EAAA,MAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,UAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,OAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,MAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA,OAVA;AAWA;AAzFA;AAlBA,CAAA","sourcesContent":["<script>\r\nimport axios from \"axios\"; \r\nexport default {\r\n\tname: 'Forum',\r\n    data() {\r\n      return {\r\n        donneesText : {\r\n          message : \"\"\r\n        },\r\n        donneesPicture : {\r\n          media : \"\"\r\n        },\r\n        posted : {\r\n            message : \"\",\r\n            name : \"\", \r\n            picture : \"\",\r\n            date : \"\"\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n        submit(e) {\r\n        //partager un post\r\n            e.preventDefault();\r\n\r\n            const params = new URLSearchParams();\r\n            params.append('userName', this.userName);\r\n            params.append('email', this.email);\r\n            params.append('password', this.password);\r\n\r\n            axios.post(\"http://localhost:3030/api/post\", params)\r\n            .then(function (response) {\r\n                console.log(response.data);\r\n                console.log(response.status);\r\n                console.log(response.statusText);\r\n                console.log(response.headers);\r\n                console.log(response.config);\r\n            })\r\n            .catch(function (error) {\r\n                this.output = error;\r\n            });\r\n        },\r\n        submitAnswer(e) {\r\n        //répondre à un post\r\n            e.preventDefault();\r\n\r\n            const params = new URLSearchParams();\r\n            params.append('userName', this.userName);\r\n            params.append('email', this.email);\r\n            params.append('password', this.password);\r\n\r\n            axios.post(\"http://localhost:3030/api/post\", params)\r\n            .then(function (response) {\r\n                console.log(response.data);\r\n                console.log(response.status);\r\n                console.log(response.statusText);\r\n                console.log(response.headers);\r\n                console.log(response.config);\r\n            })\r\n            .catch(function (error) {\r\n                this.output = error;\r\n            });\r\n        },\r\n        addPost(e) {\r\n        //editer un post  editPost\r\n            e.preventDefault();\r\n\r\n            const params = new URLSearchParams();\r\n            params.append('message', this.message);\r\n\r\n            axios.post(\"http://localhost:3030/api/post\", params)\r\n            .then(function (response) {\r\n                console.log(response.data);\r\n                console.log(response.status);\r\n                console.log(response.statusText);\r\n                console.log(response.headers);\r\n                console.log(response.config);\r\n            })\r\n            .catch(function (error) {\r\n                this.output = error;\r\n            });\r\n        },\r\n        addPicture(e) {\r\n        //editer un post  editPost\r\n            e.preventDefault();\r\n            const file = (event.target).files[0];\r\n            console.log(file);\r\n            this.media.get('image').setValue(file);\r\n            this.media.updateValueAndValidity();\r\n            const reader = new FileReader();\r\n            reader.onload = () => {\r\n                this.imagePreview = reader.result;\r\n            };\r\n            reader.readAsDataURL(file);\r\n\r\n            const params = new URLSearchParams();\r\n            params.append('media', this.media);\r\n\r\n            axios.post(\"http://localhost:3030/api/post\", params)\r\n            .then(function (response) {\r\n                console.log(response.data);\r\n                console.log(response.status);\r\n                console.log(response.statusText);\r\n                console.log(response.headers);\r\n                console.log(response.config);\r\n            })\r\n            .catch(function (error) {\r\n                this.output = error;\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n<template>\r\n  <div id=\"blocCentral\">\r\n    <div class=\"publishPost\">\r\n        <ul>\r\n            <li @click=\"submit\">\r\n                <img src=\"../../assets/quote-left-solid.png\" alt=\"icone\" title=\"icone\" />\r\n                <p>Share an update</p>\r\n            </li>\r\n            <li>\r\n                <input type=\"file\" value=\"\" @change=\"addPicture\" v-model=\"media\">\r\n                <div class=\"iconePicture\"></div>\r\n            </li>\r\n\r\n            <li @click=\"submitAnswer\">\r\n                <img src=\"../../assets/edit-solid.png\" alt=\"icone\" title=\"icone\" />\r\n                <p>Share a post</p>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n    <div class=\"blocPost\">\r\n        <input type=\"text\" placeholder=\"What's on your mind?\" v-model=\"message\">\r\n        <div class=\"moreOptions\">\r\n            <div class=\"iconePicture\"></div>\r\n            <input type=\"file\" value=\"\" @change=\"addPicture\">\r\n            <button type=\"button\" @click=\"addPost\">Poster</button>\r\n        </div>\r\n    </div>\r\n    <div class=\"filters\">\r\n        <label for=\"connections\">Connections Only</label>\r\n        <select name=\"postsFriends\" id=\"postsFriends\">\r\n            <option value=\"\"></option>\r\n            <option value=\"postsKey\">Posts Key</option>\r\n        </select>\r\n        <label for=\"popular\">Sort by Popular</label>\r\n        <!--afficher les posts les plus consultés-->\r\n        <select name=\"popular\" id=\"popular\">\r\n            <option value=\"\"></option>\r\n            <option value=\"popularPosts\">Popular Posts</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"posts\">\r\n        <div class=\"post\">\r\n            <div class=\"user\">\r\n                <img src=\"../../assets/user-circle-regular.png\" alt=\"iconeUser\" title=\"iconeUser\" />\r\n                <p>Name of User</p>\r\n                <p class=\"date\">date publication</p>\r\n            </div>\r\n            <p>Commentaire postes</p>\r\n            <button type=\"submit\">reply</button>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<style lang=\"scss\">\r\n.forum {\r\n    width: 75%;\r\n    margin-top: 40px;\r\n}\r\n.iconePicture {\r\n    content: url(\"../../assets/camera-retro-solid.png\");\r\n    width: 6%;\r\n    margin: 0 10px;\r\n}\r\n.publishPost {\r\n    width: 90%;\r\n    margin: 0 auto;\r\n    ul {\r\n        display: flex;\r\n        justify-content: space-around;\r\n        list-style: none;\r\n        li {\r\n            display: flex;\r\n            align-items: center;\r\n                flex-direction: row-reverse;\r\n            img {\r\n                width: 6%;\r\n                margin: 0 10px;\r\n            }\r\n        }\r\n    }\r\n}\r\n.blocPost {\r\n    width: 85%;\r\n    margin: 0 10%;\r\n    box-shadow: 2px 2px 2em rgb(0 0 0 / 16%);\r\n    input {\r\n        width: 95%;\r\n        height: 100px;\r\n        border: none;\r\n        padding-left: 30px;\r\n    }\r\n    .moreOptions {\r\n        display: flex;\r\n        margin-top: 10px;\r\n        .iconePicture {\r\n            content: url(\"../../assets/camera-retro-solid.png\");\r\n            width: 2%;\r\n            height: 20px;\r\n        }\r\n        input {\r\n            padding: 0;\r\n        }\r\n        button {\r\n            padding: 8px;\r\n            color: white;\r\n            text-transform: uppercase;\r\n            font-weight: bold;\r\n            background-color: #ff5722;\r\n            border: none;\r\n            margin-left: auto;\r\n        }\r\n    }\r\n}\r\n.filters {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    width: 85%;\r\n    margin: 0 auto;\r\n    label {\r\n        margin: 20px 10px;\r\n    }\r\n    select{\r\n        margin: 20px;\r\n        width: 20px;\r\n        border: none;\r\n    }\r\n}\r\n.posts {\r\n    width: 85%;\r\n    margin: 0 10%;\r\n    box-shadow: 2px 2px 2em rgb(0 0 0 / 16%);\r\n    .post {\r\n        margin: 0 auto;\r\n        background-color: white;\r\n        .user {\r\n            display: flex;\r\n            align-items: center;\r\n                justify-content: end;\r\n            img {\r\n                position: relative;\r\n                right: 15px;\r\n                bottom: 10px;\r\n                width: 3%;\r\n            }\r\n            p {\r\n                bottom: 10px;\r\n            }\r\n            p.date {\r\n                margin-left: auto;\r\n                margin-right: 10px;\r\n            }\r\n        }\r\n        button {\r\n            padding: 10px 12px;\r\n            color: white;\r\n            text-transform: uppercase;\r\n            font-weight: bold;\r\n            background-color: #ff5722;\r\n            border: none;\r\n            position: relative;\r\n            left: 42%;\r\n            top: 10px;\r\n            border-radius: 20px;\r\n        }\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}